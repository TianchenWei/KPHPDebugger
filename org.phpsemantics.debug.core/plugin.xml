<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.debug.core.sourceLocators">
      <sourceLocator
            class="org.phpsemantics.debug.core.PersistableSourceLocator1"
            id="org.phpsemantics.debug.core.sourceLocator1"
            name="name">
      </sourceLocator>
   </extension>
   <extension
         point="org.eclipse.debug.core.sourcePathComputers">
      <sourcePathComputer
            class="org.phpsemantics.debug.core.SourcePathComputerDelegate1"
            id="org.phpsemantics.debug.core.sourcePathComputer1">
      </sourcePathComputer>
   </extension>
   <extension
         point="org.eclipse.debug.core.breakpoints">
      <breakpoint
            class="org.phpsemantics.debug.core.model.KPHPLineBreakpoint"
            id="org.phpsemantics.debug.core.breakpoint1"
            markerType="org.phpsemantics.debug.core.breakpoint1">
      </breakpoint>
   </extension>
   <extension
         id="id1"
         point="org.eclipse.core.resources.markers">
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="org.phpsemantics.debug.core.launching.FakeLaunchDelegate"
            id="org.phpsemantics.debug.core.launchConfigurationType1"
            modes="debug"
            name="KPHP">
      </launchConfigurationType>
   </extension>

</plugin>
